{
    "buildCommand": null,
    "outputDirectory": ".",
    "framework": null,
    "rewrites": [
        {
            "source": "/api/gemini",
            "destination": "/api/gemini.js"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Frame-Options",
                    "value": "DENY"
                },
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "Referrer-Policy",
                    "value": "strict-origin-when-cross-origin"
                }
            ]
        }
    ]
}